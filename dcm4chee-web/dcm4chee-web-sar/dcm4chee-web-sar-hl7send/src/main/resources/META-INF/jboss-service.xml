<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server PUBLIC "-//JBoss//DTD MBean Service 4.2//EN" "http://www.jboss.org/j2ee/dtd/jboss-service_4_2.dtd">

<!--  Version $Revision:$ $Date:$ -->
<server>
  <mbean code="org.jboss.jms.server.destination.QueueService"
    name="dcm4chee.web:service=Queue,name=HL7SendV2" xmbean-dd="xmdesc/Queue-xmbean.xml">
    <depends optional-attribute-name="ServerPeer">jboss.messaging:service=ServerPeer</depends>
    <depends>jboss.messaging:service=PostOffice</depends>
  </mbean>
  
  <mbean code="org.dcm4chee.web.service.hl7.v2.HL7SendV2Service" 
         name="dcm4chee.web:service=HL7SendV2"
         xmbean-dd="resource:META-INF/xmdesc/hl7send-xmbean.xml">
    <depends optional-attribute-name="JMSServiceName">dcm4chee.archive:service=JMS</depends>
    <depends>dcm4chee.web:service=Queue,name=HL7SendV2</depends>
    <depends optional-attribute-name="TlsCfgServiceName">dcm4chee.web:service=TlsConfig</depends>
    <depends optional-attribute-name="ContentEditServiceName">dcm4chee.web:service=ContentEditService</depends>
    <depends optional-attribute-name="TemplatesServiceName">dcm4chee.archive:service=Templates</depends>

  </mbean>
</server>